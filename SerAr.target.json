{
    "name" : "AllArchives" ,
    "target_type" : "library",
    "parameters" : [
        "INTERFACE"
    ],
    "include_directories" : {
        "interface" : [ 
            "$<BUILD_INTERFACE:${CMAKE_CURRENT_SOURCE_DIR}/include>",
            "$<INSTALL_INTERFACE:include>" ]
    },
    "link_libraries" : {
        "interface": [
            "SerAr::Core",
            "MyCEL::MyCEL",
            "$<$<BOOL:SERAR_WITH_CONFIGFILE>:$<TARGET_NAME_IF_EXISTS:SerAr::ConfigFile>>",
            "$<$<BOOL:SERAR_WITH_PROGRAMOPTIONS>:$<TARGET_NAME_IF_EXISTS:SerAr::ProgramOptionsWrapper>>",
            "$<$<BOOL:SERAR_WITH_MATLAB>:$<TARGET_NAME_IF_EXISTS:SerAr::MATLAB>>",
            "$<$<BOOL:SERAR_WITH_HDF5>:$<TARGET_NAME_IF_EXISTS:SerAr::HDF5>>",
            "$<$<BOOL:SERAR_WITH_CEREAL>:$<TARGET_NAME_IF_EXISTS:SerAr::CerealWrappers>>",
            "$<$<BOOL:SERAR_WITH_JSON>:$<TARGET_NAME_IF_EXISTS:SerAr::JSON>>"
        ]
    },
    "compile_features" : {
        "interface" : [ 
            "cxx_std_20"
        ]
    },
    "compile_definitions" : {
        "interface" : [
            "$<$<BOOL:SERAR_WITH_CONFIGFILE>:SERAR_HAS_CONFIGFILE>",
            "$<$<BOOL:SERAR_WITH_PROGRAMOPTIONS>:SERAR_HAS_PROGRAMOPTIONS>",
            "$<$<BOOL:SERAR_WITH_MATLAB>:SERAR_HAS_MATLAB>",
            "$<$<BOOL:SERAR_WITH_HDF5>:SERAR_HAS_HDF5>",
            "$<$<BOOL:SERAR_WITH_CEREAL>:SERAR_HAS_CEREAL>",
            "$<$<BOOL:SERAR_WITH_JSON>:SERAR_HAS_JSON>"
        ]
    },
    "public_headers" : ["include/SerAr.hpp", "include/AllArchiveIncludes.hpp", "include/AllArchiveEnums.hpp"]
}

